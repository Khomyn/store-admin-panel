{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "admin",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "admin",
            "kebab-case"
        ],
        "no-console": [
          true,
          "log",
          "debug",
          "info",
          "time",
          "timeEnd",
          "trace"
        ],
        "no-shadowed-variable": true,
        "no-string-literal": false
    }
}
